@model IEnumerable<backenddb_c.Models.Bitacora>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NumTransaccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NomTabla)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NomCampo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NuevoValor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorAnterior)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UsuarioTransaccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaTransaccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoTransaccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LlavePrimaria)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NumTransaccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NomTabla)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NomCampo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NuevoValor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorAnterior)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UsuarioTransaccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaTransaccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoTransaccion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LlavePrimaria)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
